<% losts_of_products = Product.includes(:user).all %>
<table >
<tr>
  <td> <b>Product </b> </td>
  <td> <b>Price</b> </td>
  <td> <b>Seller</b> </td>
</tr>

  <% losts_of_products.each do |product| %>
  <tr>
    <td>
    "<%= product.name %>"
  </td>
  <td>
  "$ <%= product.price %>"
</td>
<td>
    <%= product.user.name %>
  </td>
  </tr>
  <% end %>
</table>
